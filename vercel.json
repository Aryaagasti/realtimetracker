{
    "name": "my-vercel-project",
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/views/$1"
      }
    ],
    "builds": [
      {
        "src": "/public/**",
        "use": "@vercel/static"
      }
    ]
  }
  